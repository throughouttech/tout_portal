<p id="notice"><%= notice %></p>

<h1>Admins</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <%=link_to "New Admin",new_admin_registration_path%>
  </br>
    <% @admin.each do |admin| id = admin.id %>
      <tr>
      <td><%= link_to admin.email, admin_admin_path(id) %></td>
      <td><%= link_to 'Destroy', admin_admin_path(id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    <% end %>
  </tbody>
</table>
<br>
