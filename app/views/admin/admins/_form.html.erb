<div class="container">
  <div class="row" style="margin-top:20px">
    <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3">
     <%= form_for [:admin, @admin] do |f| %>
        <fieldset>
          <div class="form-group" >
            <label class="col-lg-6 control-label ">Email:</label>
            <div class="col-lg-6 ">
              <%= f.email_field :email, autofocus: true, class: 'form-control input-lg' %>
            </div>
          </div>
      </fieldset>
      </br>
      <button type="f.submit" class="btn btn-primary">Create Admin</button>
    <% end %>
  </div>
</div>
</br>