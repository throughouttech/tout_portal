<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <%=link_to "New Users", new_admin_user_path %>
  </br>
    <% @user.each do |user| id = user.id %>
      <tr>
      <td><%= link_to user.email, admin_user_path(id) %></td>
      <td><%= link_to 'Destroy', admin_user_path(id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    <% end %>
  </tbody>
</table>
<br>
